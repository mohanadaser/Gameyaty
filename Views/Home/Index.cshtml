<!------- طرق الدفع والتبرعات-->
<div class="container">

    <!-- Button to Open the Modal -->
    <!-- The Modal -->
    <div class="modal" id="myModal">
        <div class="modal-dialog">
            <div class="modal-content">

                <!-- Modal Header -->
                <div class="modal-header">
                    <h4 class="modal-title">طرق التبرعات وطرق الدفع المختلفه</h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>

                <!-- Modal body -->
                <div class="modal-body">
                    <h4>طرق التبرعات</h4>
                    <div class="row">

                        <div class="col-md-4">
                            <div class="text-center">

                                <img src="~/img/Donation/blood.png" alt="" class="img-fluid" width="100" height="100" />
                                <h4>التبرع بالدم</h4>
                            </div>
                            <div class="text-center">
                                <img src="~/img/Donation/sadaka.jpg" alt="" class="img-fluid" width="100" height="100" />
                                <h4>صدقه جاريه</h4>
                            </div>

                        </div>
                        <div class="col-md-4">

                            <div class="text-center">
                                <img src="~/img/Donation/zakat.jpg" alt="" class="img-fluid" width="100" height="100" />
                                <h4>ذكاة مال</h4>
                            </div>
                            <div class="text-center">
                                <img src="~/img/Donation/companies.jpg" alt="" class="img-fluid" width="100" height="100" />
                                <h4>شركات</h4>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="text-center">
                                <img src="~/img/Donation/yatm.png" alt="" class="img-fluid" width="100" height="150" />
                                <h4>كفالة يتيم</h4>
                            </div>
                        </div>


                    </div>

                    <hr />
                    <h4>طرق الدفع</h4>
                    <img src="~/img/Donation/bank.png" alt="" class="img-fluid" width="100" height="100" />

                    <img src="~/img/Donation/post.jpg" alt="" class="img-fluid" width="100" height="100" />
                    <img src="~/img/Donation/vodafone-cash-550x550w.png" alt="" class="img-fluid" width="100" height="100" />
                </div>

                <!-- Modal footer -->
                <div class="modal-footer">
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
                </div>

            </div>
        </div>
    </div>

</div>

