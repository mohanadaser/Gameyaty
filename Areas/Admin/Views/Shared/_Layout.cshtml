                        <input type="number" class="form-control-md" placeholder="......رقم التليفون" />
                        <br />
                        <br />

                        <input type="email" class="form-control-range" placeholder="......اكتب الايميل" />
                        <br />
                        <textarea class="form-control-range" placeholder="......اكتب فكرتك"></textarea>
                        <br />
                        <button class="btn btn-primary form-control-range">ارسال</button>
                    </form>

                </div>

            </div>

        </div>
    </div>
    <!-------------------------------------------------------------------->
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <div class="footer">
        <div class="container">
            <div class="row">
